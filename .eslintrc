{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "prettier/prettier": 2,
    "no-undef": 0,
    "@typescript-eslint/explicit-function-return-type": 0,
    "no-constant-condition": 0
  }
}
